{
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.detectIndentation": true,
  "workbench.colorTheme": "Dracula",
  "workbench.iconTheme": "vscode-icons",
  "window.zoomLevel": 0,
  "window.menuBarVisibility": "toggle",
  "files.watcherExclude": {
      "**/.git/objects/**": true,
      "**/.git/subtree-cache/**": true,
      "**/node_modules/*/**": true
    }
}
